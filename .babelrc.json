{
    "plugins": [
        "@babel/plugin-transform-class-properties",
        "@babel/plugin-transform-object-rest-spread"
    ],
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false
            }
        ],
        "@babel/preset-react"
    ],
    "env": {
        "production": {
            "plugins": ["transform-react-remove-prop-types"]
        }
    }
}
